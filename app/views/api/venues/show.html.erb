<%= render "shared/admin_nav" %>
<div class="panel panel-default">
	<!-- Default panel contents -->
	<div class="panel-heading">Venue : <%= @venue.name %></div>
	<div class="panel-body">
		<% if notice %>
			<div class="panel-body">
				<p id="notice"><%= notice %></p>
			</div>
		<% end %>

		<p>
		  <strong>Name:</strong>
		  <%= @venue.name %>
		</p>
		<p>
		  <strong>Concerts:</strong><br/>
		  <%= raw @venue.concerts.collect{|c| c.show_date.strftime("On %m/%d/%Y at %I:%M%p")}.join("<br/>") %>
		</p>
	</div>
	<div class="panel-footer">
		<%= link_to 'Edit', edit_api_venue_path(@venue) %> |
		<%= link_to 'Back', api_venues_path %>
	</div>
</div>
